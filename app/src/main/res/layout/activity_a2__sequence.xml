<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".A2_Sequence">

    <Button
        android:id="@+id/UI_a2_CavanCity"
        android:layout_width="96dp"
        android:layout_height="74dp"
        android:text="Cavan City"
        app:layout_constraintBottom_toTopOf="@+id/UI_a2_Center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75" />

    <Button
        android:id="@+id/UI_a2_DublinCity"
        android:layout_width="96dp"
        android:layout_height="74dp"
        android:text="Dublin City"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toEndOf="@+id/UI_a2_Center"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/UI_a2_MayoCity"
        android:layout_width="96dp"
        android:layout_height="74dp"
        android:text="Mayo City"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UI_a2_Center"
        app:layout_constraintVertical_bias="0.25" />

    <Button
        android:id="@+id/UI_a2_Center"
        android:layout_width="96dp"
        android:layout_height="74dp"
        android:text="Center"
        tools:layout_editor_absoluteX="317dp"
        tools:layout_editor_absoluteY="168dp" />

    <Button
        android:id="@+id/UI_a2_SligoCity"
        android:layout_width="96dp"
        android:layout_height="74dp"
        android:text="Sligo City"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/UI_a2_Center"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>